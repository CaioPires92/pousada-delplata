{
    "name": "web",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "prisma:generate": "node ./node_modules/prisma/build/index.js generate",
        "dev": "npm run prisma:generate && next dev --webpack -p 3001",
        "build": "npm run prisma:generate && next build --webpack",
        "start": "next start --webpack",
        "lint": "eslint",
        "typecheck": "tsc --noEmit",
        "test": "vitest run",
        "deploy:zip:lean": "powershell -ExecutionPolicy Bypass -File ./scripts/create-hostinger-zip.ps1 -OutputZip deploy-hostinger-lean.zip -OptimizeImages",
        "deploy:zip:lean:fast": "powershell -ExecutionPolicy Bypass -File ./scripts/create-hostinger-zip.ps1 -OutputZip deploy-hostinger-lean.zip -SkipChecks -OptimizeImages",
        "deploy:zip:allphotos:fast": "powershell -ExecutionPolicy Bypass -File ./scripts/create-hostinger-zip.ps1 -OutputZip deploy-hostinger-allphotos.zip -SkipChecks -OptimizeImages -KeepAllPhotos",
        "hostinger:install": "powershell -ExecutionPolicy Bypass -File ./scripts/hostinger.ps1 -Action install",
        "hostinger:build": "powershell -ExecutionPolicy Bypass -File ./scripts/hostinger.ps1 -Action build",
        "hostinger:start": "powershell -ExecutionPolicy Bypass -File ./scripts/hostinger.ps1 -Action start",
        "hostinger:zip:lean": "powershell -ExecutionPolicy Bypass -File ./scripts/hostinger.ps1 -Action zip-lean",
        "hostinger:zip:lean:fast": "powershell -ExecutionPolicy Bypass -File ./scripts/hostinger.ps1 -Action zip-lean-fast",
        "hostinger:zip:allphotos:fast": "powershell -ExecutionPolicy Bypass -File ./scripts/hostinger.ps1 -Action zip-allphotos-fast",
        "hostinger:full:lean": "powershell -ExecutionPolicy Bypass -File ./scripts/hostinger.ps1 -Action full-lean",
        "hostinger:full:allphotos": "powershell -ExecutionPolicy Bypass -File ./scripts/hostinger.ps1 -Action full-allphotos",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:responsive": "node scripts/responsive-smoke.js",
        "postinstall": "npm run prisma:generate",
        "db:update": "npx tsx scripts/manage-pousada.ts --action=update-desc --dry-run",
        "db:update:desc": "npx tsx scripts/manage-pousada.ts --action=update-desc --confirm=YES",
        "db:update:desc:dry": "npx tsx scripts/manage-pousada.ts --action=update-desc --dry-run",
        "db:update:prices": "npx tsx scripts/manage-pousada.ts --action=update-prices --price=0.10 --confirm=YES",
        "db:update:prices:dry": "npx tsx scripts/manage-pousada.ts --action=update-prices --price=0.10 --dry-run",
        "db:update:fees": "npx tsx scripts/manage-pousada.ts --action=update-fees --extra=100 --child=80 --confirm=YES",
        "db:update:fees:dry": "npx tsx scripts/manage-pousada.ts --action=update-fees --extra=100 --child=80 --dry-run",
        "diag:mp:credentials": "node scripts/diagnostics/check-mp-credentials.js",
        "diag:payments": "node scripts/diagnostics/check-payments.js",
        "diag:booking:last": "node scripts/diagnostics/get-latest-booking.js",
        "diag:mp:config": "node scripts/diagnostics/test-mp-config.js",
        "diag:db:inspect": "npx tsx scripts/diagnostics/test-db.ts",
        "diag:legacy:config": "node scripts/diagnostics/run-tests.js",
        "diag:legacy:db": "node scripts/diagnostics/test-database.js",
        "diag:legacy:mp": "node scripts/diagnostics/test-mercadopago.js",
        "diag:legacy:all": "node scripts/diagnostics/run-all-tests.js",
        "diag:legacy:e2e": "node scripts/diagnostics/test-system-complete.js",
        "diag:legacy:responsive": "node scripts/diagnostics/test-responsiveness.js",
        "diag:photos": "node scripts/diagnostics/test-photos.js",
        "diag:create-test-user": "node scripts/diagnostics/create-test-user.js"
    },
    "dependencies": {
        "@gsap/react": "^2.1.2",
        "@libsql/client": "^0.8.1",
        "@prisma/adapter-libsql": "^5.22.0",
        "@radix-ui/react-popover": "^1.1.15",
        "@radix-ui/react-slot": "^1.2.4",
        "@radix-ui/react-tooltip": "^1.2.7",
        "@sentry/nextjs": "^10.37.0",
        "@upstash/ratelimit": "^2.0.5",
        "@upstash/redis": "^1.34.9",
        "bcryptjs": "^3.0.3",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "dotenv-cli": "^11.0.0",
        "framer-motion": "^12.23.24",
        "gsap": "^3.13.0",
        "jose": "^6.1.0",
        "jsonwebtoken": "^9.0.2",
        "lucide-react": "^0.555.0",
        "mercadopago": "^2.10.0",
        "next": "16.0.7",
        "nodemailer": "^7.0.10",
        "react": "19.2.0",
        "react-day-picker": "^9.13.0",
        "react-dom": "19.2.0",
        "tailwind-merge": "^3.4.0"
    },
    "devDependencies": {
        "@prisma/client": "^5.22.0",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.1",
        "@testing-library/user-event": "^14.6.1",
        "@types/bcryptjs": "^2.4.6",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^20",
        "@types/nodemailer": "^7.0.4",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@vitejs/plugin-react": "^5.1.2",
        "@vitest/coverage-v8": "^4.0.16",
        "autoprefixer": "^10.4.22",
        "baseline-browser-mapping": "^2.9.18",
        "eslint": "^9",
        "eslint-config-next": "16.0.7",
        "jsdom": "^27.4.0",
        "postcss": "^8.5.6",
        "prisma": "^5.22.0",
        "puppeteer": "^24.31.0",
        "sharp": "^0.34.5",
        "tailwindcss": "^3.4.17",
        "tailwindcss-animate": "^1.0.7",
        "tsx": "^4.21.0",
        "typescript": "^5",
        "vitest": "^4.0.16",
        "vitest-mock-extended": "^3.1.0"
    },
    "prisma": {
        "seed": "node prisma/seed.js"
    }
}
